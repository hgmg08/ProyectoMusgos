<?php

namespace entities;

use Doctrine\ORM\Mapping as ORM;

/**
 * entities\Sustrato
 */
class Sustrato
{
    /**
     * @var string $name
     */
    private $name;

    /**
     * @var integer $id
     */
    private $id;

    /**
     * @var entities\Habito
     */
    private $habito;

    /**
     * @var \Doctrine\Common\Collections\ArrayCollection
     */
    private $taxons;

    public function __construct()
    {
        $this->taxons = new \Doctrine\Common\Collections\ArrayCollection();
    }
    
    /**
     * Set name
     *
     * @param string $name
     * @return Sustrato
     */
    public function setName($name)
    {
        $this->name = $name;
        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set habito
     *
     * @param entities\Habito $habito
     * @return Sustrato
     */
    public function setHabito(\entities\Habito $habito = null)
    {
        $this->habito = $habito;
        return $this;
    }

    /**
     * Get habito
     *
     * @return entities\Habito 
     */
    public function getHabito()
    {
        return $this->habito;
    }

    /**
     * Add taxons
     *
     * @param entities\Taxon $taxons
     * @return Sustrato
     */
    public function addTaxon(\entities\Taxon $taxons)
    {
        $this->taxons[] = $taxons;
        return $this;
    }

    /**
     * Get taxons
     *
     * @return Doctrine\Common\Collections\Collection 
     */
    public function getTaxons()
    {
        return $this->taxons;
    }
}